{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug (MinGW)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:QT_DIR}/bin;${env:MINGW_PATH};${env:PATH}"
