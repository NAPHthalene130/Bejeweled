            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "${env:MINGW_PATH}/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "envFile": "${workspaceFolder}/.vscode/.env",
